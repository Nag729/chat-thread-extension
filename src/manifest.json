{
  "name": "Google Chat Threads",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Display Thread Link in Google Chat",
  "content_scripts": [
    {
      "matches": ["https://chat.google.com/u/2/room/*"],
      "js": ["js/chunk-vendors.js", "js/app.js"],
      "css": ["css/app.css"]
    }
  ]
}
